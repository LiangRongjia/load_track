<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
        android:background="#F5F5F5"
    tools:context=".Activity.GaodeActivity">

    <EditText
        android:id="@+id/editText"
        android:layout_width="109dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="12dp"
        android:background="#F1FFFFFF"
        android:gravity="center"
        android:hint="起点"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="12dp"
        android:layout_height="2dp"
        android:layout_marginTop="32dp"
        android:background="#111111"
        android:gravity="center"
        app:layout_constraintStart_toEndOf="@+id/editText"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="109dp"
        android:layout_height="40dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:background="#F1FFFFFF"
        android:gravity="center"
        android:hint="终点"
        app:layout_constraintStart_toEndOf="@+id/editText"
        app:layout_constraintTop_toTopOf="@+id/editText" />

    <Button
        android:id="@+id/button3"
        android:layout_width="89dp"
        android:layout_height="38dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="12dp"
        android:background="#D3D6D8"
        android:hint="确定"
        app:layout_constraintStart_toEndOf="@+id/editText2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="367dp"
        android:layout_height="2dp"
        android:layout_marginTop="4dp"
        android:background="#FFFDFD"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3"></TextView>

    <TextView
        android:id="@+id/textView"
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:hint="车辆信息"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText"></TextView>

    <TextView
        android:id="@+id/carView"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:gravity="center"
        android:hint="你好"
        android:padding="0dp"
        android:textColor="#0A0A0A"
        android:textSize="25dp"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:layout_width="2dp"
        android:layout_height="86dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="4dp"
        android:background="#FFFDFD"
        app:layout_constraintStart_toEndOf="@+id/carView"
        app:layout_constraintTop_toBottomOf="@+id/editText2"></TextView>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="110dp"
        android:layout_height="20dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:gravity="center"
        android:hint="距离"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="@+id/textView"></TextView>

    <TextView
        android:id="@+id/mileView"
        android:layout_width="110dp"
        android:layout_height="60dp"
        android:gravity="center"
        android:textColor="#0A0A0A"
        android:textSize="25dp"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:layout_width="2dp"
        android:layout_height="86dp"
        android:layout_marginTop="4dp"
        android:background="#FFFDFD"
        app:layout_constraintStart_toEndOf="@+id/mileView"
        app:layout_constraintTop_toBottomOf="@+id/editText2"></TextView>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="60dp"
        android:layout_height="20dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:gravity="center"
        android:hint="车辆编号"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/textView2" />

    <TextView
        android:id="@+id/blockView"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:gravity="center"
        android:textColor="#0A0A0A"
        android:textSize="25dp"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3"></TextView>

    <TextView
        android:layout_width="367dp"
        android:layout_height="2dp"
        android:background="#FFFDFD"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/blockView"></TextView>


    <TextView
        android:id="@+id/textView4"
        android:layout_width="360dp"
        android:layout_height="20dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:gravity="center"
        android:hint="时间"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mileView">

    </TextView>

    <TextView
        android:id="@+id/timeView"
        android:layout_width="360dp"
        android:layout_height="40dp"
        android:gravity="center"
        android:textSize="25dp"
        android:textColor="#0A0A0A"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="367dp"
        android:layout_height="2dp"
        android:background="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/timeView"></TextView>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="360dp"
        android:layout_height="20dp"
        android:gravity="center"
        android:hint="轨迹编号"
        app:layout_constraintStart_toStartOf="@+id/timeView"
        app:layout_constraintTop_toBottomOf="@+id/timeView"></TextView>

    <TextView
        android:id="@+id/hashView"
        android:layout_width="360dp"
        android:layout_height="60dp"
        android:gravity="center"
        android:textSize="25dp"
        android:textColor="#0A0A0A"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <!--    <TextView-->

<!--        android:layout_width="406dp"-->
<!--        android:layout_height="389dp"-->
<!--        android:background="@android:color/holo_blue_dark"-->
<!--        android:gravity="center"-->
<!--        android:hint="地图显示"-->
<!--        app:layout_constraintStart_toStartOf="parent"-->
<!--        app:layout_constraintTop_toBottomOf="@+id/hashView"></TextView>-->

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="76dp"
        android:background="#B1ADAD"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"></TextView>

    <com.amap.api.maps.MapView
        android:id="@+id/map"
        android:layout_width="368dp"
        android:layout_height="443dp"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hashView">

    </com.amap.api.maps.MapView>

</androidx.constraintlayout.widget.ConstraintLayout>
